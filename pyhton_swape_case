def swap_case(s):
    swapped_chars = []
    for char in s:
        if char.islower():
            swapped_chars.append(char.upper())
        elif char.isupper():
            swapped_chars.append(char.lower())
        else:
            swapped_chars.append(char)  
    return ''.join(swapped_chars)
    

if __name__ == '__main__':
    s = input()
    result = swap_case(s)
    print(result)
