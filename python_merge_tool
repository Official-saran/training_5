def merge_the_tools(string, k):
    n = len(string)
    for i in range(0, n, k):
        substring = string[i:i+k]  
        seen = set()
        output = []
        for char in substring:
            if char not in seen:
                seen.add(char)
                output.append(char)
        print(''.join(output))


if __name__ == '__main__':
    string, k = input(), int(input())
    merge_the_tools(string, k)
